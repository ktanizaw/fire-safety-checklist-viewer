{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "lint": "npm-run-all --parallel lint:*",
    "lint:es": "eslint --cache --cache-location ./node_modules/.cache/eslint --ext .ts,.js,.vue src",
    "lint:prettier": "prettier --check '**/*.{js,ts,vue,scss,css,html,json,yml,md}'",
    "lint:style": "stylelint --cache --cache-location ./node_modules/.cache/stylelint 'src/**/*.{css,scss,vue}'",
    "lintfix": "npm-run-all --parallel lintfix:*",
    "lintfix:es": "npm run lint:es -- --fix",
    "lintfix:prettier": "npm run lint:prettier -- --write",
    "lintfix:style": "npm run lint:style -- --fix"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.8.0",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^2.0.0",
    "@nuxt/image": "^1.11.0",
    "@urql/vue": "^2.0.0",
    "@vueuse/core": "^13.6.0",
    "date-fns": "^4.1.0",
    "eslint": "^9.33.0",
    "floating-vue": "^5.2.2",
    "graphql": "^16.11.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^4.0.3",
    "nuxt-typed-router": "^4.0.1",
    "prettier": "^3.6.2",
    "urql": "^5.0.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-urql": "^4.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.8.0",
    "sass-embedded": "^1.90.0",
    "stylelint": "^16.23.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.12.1"
  }
}
